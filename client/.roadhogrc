{
  "entry": "src/index.js",
  "disableCSSModules": true,
  "disableCSSSourceMap": true,
  "env": {
    "development": {
      "extraBabelPlugins": [
        "transform-runtime",
        "dva-hmr",
        [ "import", { "libraryName": "antd-mobile", "style": true } ]
